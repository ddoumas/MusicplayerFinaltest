<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicplayerfinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/search" />
        <activity
            android:name=".PlaylistsActivity"
            android:label="@string/playlists" />

        <activity
            android:name=".RadioActivity"
            android:label="@string/radio"></activity>
        <activity
            android:name=".PayActivity"
            android:label="@string/creditcard_buy"></activity>
    </application>

</manifest>